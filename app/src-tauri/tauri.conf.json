{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Boer DashBert",
  "version": "0.9.0",
  "identifier": "com.insomniacs.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Boer DashBert",
        "width": 1000,
        "height": 600,
        "minWidth": 1000,
        "minHeight": 600,
        "theme": "Dark"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
